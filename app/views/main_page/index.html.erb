                      <div class="modal-dialog modal-lg" role="document">
                        <div class="modal-content">
                            <%= image_tag "http://www.mikwood.by/pic/cert/cert5_2.jpg",
                                    class: "img-responsive img-center center-block" %>
                            <%= image_tag "http://www.mikwood.by/pic/cert/cert6_2.jpg",
                                    class: "img-responsive img-center center-block" %>
                        </div>
                      </div>
                    </div>
                <!-- Оставить заявку -->
                <button type="button" class="btn btn-large btn-success"><a href="#contact" style="color: white">ОСТАВИТЬ ЗАЯВКУ</a></button>
